{
	"bin_expo": {
  "prefix": "binpow",
  "body": [
    "long long binpow(long long a, long long b, long long m) { // (a^b)mod m ",
    "    a %= m;",
    "    long long res = 1;",
    "    while (b > 0) {",
    "        if (b & 1)",
    "            res = res * a % m;",
    "        a = a * a % m;",
    "        b >>= 1;",
    "    }",
    "    return res;",
    "}"
  ],
  "description": "bin_expo"
}

